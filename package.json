{
  "name": "@api3/operations",
  "license": "MIT",
  "private": false,
  "version": "0.0.1-d",
  "description": "A package to parse API3 operations metadata",
  "main": "./dist/index",
  "types": "./dist/types",
  "files": [
    "dist",
    "data",
    "chain"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "create-boilerplate": "ts-node src/create-boilerplate.ts",
    "create-config": "ts-node src/create-config.ts",
    "create-airseeker-config": "ts-node src/create-airseeker-config.ts",
    "export-ops-json-payloads": "ts-node src/export-ops-json-payloads.ts",
    "import-receipt": "ts-node src/import-receipt.ts",
    "eth:deploy": "cd chain && hardhat deploy --network $NETWORK",
    "deploy-all": "yarn deploy-airnode; yarn deploy-airkeeper",
    "deploy-airkeeper": "yarn ts-node src/deploy-airkeeper",
    "deploy-airnode": "yarn ts-node src/deploy-airnode",
    "generate-gateway-key": "ts-node src/generate-gateway-key.ts",
    "generate-templateIds": "ts-node src/generate-templateIds.ts",
    "generate-topup-wallets": "ts-node src/generate-topup-wallets.ts",
    "register-subscriptions": "ts-node src/register-subscriptions.ts",
    "make": "yarn run clean && yarn run compile",
    "clean": "rimraf -rf ./dist *.tgz",
    "compile": "tsc -p tsconfig.json",
    "normalize-and-validate": "ts-node src/normalize-and-validate.ts",
    "validate": "ts-node src/validate.ts",
    "normalize": "ts-node src/normalize.ts",
    "lint": "yarn run prettier:check && yarn run lint:eslint",
    "lint:eslint": "eslint . --ext .js,.ts",
    "prettier:check": "prettier --check \"./**/*.{js,ts,md,json}\"",
    "prettier:write": "prettier --write \"./**/*.{js,ts,md,json}\"",
    "test": "jest --verbose --runInBand --bail",
    "pack": "yarn pack",
    "start": "ts-node src/index.ts",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/api3dao/operations.git"
  },
  "devDependencies": {
    "@api3/airkeeper": "0.0.6",
    "@api3/airnode-abi": "^0.6.2",
    "@api3/airnode-admin": "^0.6.2",
    "@api3/airnode-node": "^0.6.2",
    "@api3/airnode-ois": "^0.6.2",
    "@api3/airnode-protocol": "^0.6.2",
    "@api3/airnode-protocol-v1": "0.5.0",
    "@ethersproject/experimental": "5.6.0",
    "@nomiclabs/hardhat-ethers": "^2.0.5",
    "@nomiclabs/hardhat-etherscan": "^3.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.2",
    "@types/prompts": "^2.0.14",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.5.0",
    "eslint-plugin-functional": "^4.0.2",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.3.0",
    "ethereum-waffle": "^3.4.4",
    "ethers": "^5.6.4",
    "hardhat": "^2.9.3",
    "hardhat-deploy": "^0.11.4",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "prettier": "^2.5.1",
    "prompts": "^2.4.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.6.4",
    "zod": "^3.14.2"
  },
  "dependencies": {}
}
